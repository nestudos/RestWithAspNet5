services:
  restwithaspnet5:
    build:
      context: C:\Cursos\AspNetCore\RestWithAspNet5\18_RestWithAspNet5_Docker_Suport\RestWithAspNet5
      dockerfile: RestWithAspNet5/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: RestWithAspNet5
      target: base
    container_name: RestWithAspNet5
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: restwithaspnet5:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/RestWithAspNet5.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    tty: true
    volumes:
    - C:\Cursos\AspNetCore\RestWithAspNet5\18_RestWithAspNet5_Docker_Suport\RestWithAspNet5\RestWithAspNet5:/app:rw
    - C:\Users\NAYTON\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\NAYTON\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\NAYTON\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\NAYTON\.nuget\packages:/root/.nuget/packages:ro
    - C:\Cursos\AspNetCore\RestWithAspNet5\18_RestWithAspNet5_Docker_Suport\RestWithAspNet5:/src:rw
version: '3.4'